{
  "name": "html2xlsx",
  "version": "0.4.0",
  "description": "Transform html to excel (just support xlsx)",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js src test",
    "build": "rimraf lib && NODE_ENV=production babel --out-dir=lib src",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test nyc mocha --no-timeouts",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.1.4",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^4.0.0",
    "coveralls": "^2.11.14",
    "eslint": "^4.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.1.2",
    "nyc": "^11.0.3",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.4",
    "stream-equal": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/html2xlsx.git"
  },
  "engines": {
    "node": ">= 4"
  },
  "keywords": [
    "xlsx",
    "excel",
    "html",
    "table"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/html2xlsx/issues"
  },
  "homepage": "https://github.com/d-band/html2xlsx#readme",
  "dependencies": {
    "better-xlsx": "^0.3.0",
    "cheerio": "^1.0.0-rc.1",
    "cssstyle": "^0.2.37",
    "juice": "^4.0.2",
    "moment": "^2.17.1",
    "tinycolor2": "^1.4.1"
  }
}
